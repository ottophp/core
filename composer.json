{
    "name": "ottophp/core",
    "autoload": {
        "psr-4": {
            "Otto\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Otto\\": "./tests/",
            "FakeProject\\": "./tests/fake-project/src"
        }
    },
    "require": {
        "capsule/di": "^3.0",
        "payload-interop/payload-interop": "^1.0",
        "pmjones/auto-route": "^2.0",
        "qiq/qiq": "^1.0@dev",
        "qiq/helper-sapien": "^1.0",
        "sapien/sapien": "^1.0"
    },
    "require-dev": {
        "pds/skeleton": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "stan": "./vendor/bin/phpstan analyze -c phpstan.neon src"
    }
}
